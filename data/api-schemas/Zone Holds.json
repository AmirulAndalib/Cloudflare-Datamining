	"POST /zones/{zone_id}/hold": {
		"description": "Enforce a zone hold on the zone, blocking the creation and activation of zones with this zone's hostname.",
		"operationId": "zones-0-hold-post",
		"parameters": [
			{
				"description": "Zone ID",
				"in": "path",
				"name": "zone_id",
				"required": true,
