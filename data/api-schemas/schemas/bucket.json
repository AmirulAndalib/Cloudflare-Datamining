{
	"description": "A single R2 bucket",
	"properties": {
		"creation_date": {
			"description": "Creation timestamp",
			"type": "string"
		},
		"location": {
			"description": "Location of the bucket",
			"enum": [
				"apac",
				"eeur",
				"enam",
				"weur",
				"wnam"
			],
			"type": "string"
		},
		"name": {
			"description": "Name of the bucket",
			"example": "example-bucket",
			"maxLength": 64,
			"minLength": 3,
			"pattern": "^[a-z0-9][a-z0-9-]*[a-z0-9]",
			"type": "string"
		}
	},
	"type": "object"
}