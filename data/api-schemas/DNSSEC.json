	"PATCH /zones/{zone_identifier}/dnssec": {
		"description": "Enable or disable DNSSEC.",
		"operationId": "dnssec-edit-dnssec-status",
		"parameters": [
			{
				"in": "path",
				"name": "zone_identifier",
				"required": true,
