																"target": null,
																"uri": null
															},
															"value": " Learn about the certifications",
															"marks": [],
															"content": null
														}
													]
												},
												{
													"nodeType": "text",
													"data": {
														"target": null,
														"uri": null
													},
													"value": " that help us preserve that security.",
													"marks": [],
