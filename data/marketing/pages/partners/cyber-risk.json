						"quoteCarousel": [
							{
								"contentTypeId": "bladeQuote",
								"quote": "When Cloudflare's technology is paired with insurance, we are able to protect businesses in an entirely new way. We are excited to offer Cloudflare customers enhanced cyber insurance coverage alongside Coalition's active security monitoring platform to help businesses build true cyber resilience with an always-on insurance policy.",
								"quoteAuthor": "Joshua Motta",
								"quoteTitle": "Co-founder and CEO, Coalition",
								"learnMoreText": null,
