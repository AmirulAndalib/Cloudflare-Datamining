							"type": "script",
							"src": "./dist/eager/installHelper.js"
						}
					]
				},
				"options": {
					"properties": {
						"buttonLocation": {
							"format": "element",
							"order": 3,
							"showIf": {
								"trigger": "button-click"
							},
							"title": "Button location",
							"type": "object"
						},
						"buttonText": {
							"default": "Start guided tour...",
							"order": 4,
							"showIf": {
								"trigger": "button-click"
							},
							"title": "Button text",
							"type": "string"
						},
						"scrollTo": {
							"description": "Should the page be scrolled to show steps when they open?",
							"order": 5,
							"title": "Scroll To",
							"type": "boolean"
						},
						"steps": {
							"default": [
								{
									"attachToDirection": "bottom",
									"attachToSelector": "h1",
									"text": "This is an example step.",
									"title": "Welcome"
								}
							],
							"description": "Create a guided tour by creating steps anchored to the parts of your site you want to call attention. Each tour step points to a specific part of your page.",
							"items": {
								"properties": {
									"attachToDirection": {
										"default": "bottom",
										"description": "Side of the anchor to attach the tour step",
										"enum": [
											"top",
											"right",
											"bottom",
											"left"
										],
										"enumNames": {
											"bottom": "Bottom",
											"left": "Left",
											"right": "Right",
											"top": "Top"
										},
										"order": 2,
										"title": "Direction",
										"type": "string"
									},
									"attachToSelector": {
										"description": "Part of the page to attach the tour step",
										"format": "selector",
										"order": 1,
										"title": "Anchor",
										"type": "string"
									},
									"showCancelLink": {
										"default": true,
										"order": 5,
										"title": "Show cancel button",
										"type": "boolean"
									},
									"text": {
										"format": "textarea",
										"maxLength": 200,
										"order": 4,
										"title": "Body",
										"type": "string",
										"validationMessage": "The step text cannot be longer than 200 characters."
									},
									"title": {
										"maxLength": 50,
										"order": 3,
										"title": "Header",
										"type": "string",
										"validationMessage": "The step title cannot be longer than 50 characters."
									}
								},
								"title": "Step",
								"type": "object"
							},
							"maxItems": 10,
							"order": 6,
							"title": "Steps",
							"type": "array"
						},
						"theme": {
							"default": "default",
							"description": "Sets the overall styling of the tour",
							"enum": [
								"default",
								"dark",
								"square",
								"square-dark"
							],
							"enumNames": {
								"dark": "Dark",
								"default": "Default",
								"square": "Square",
								"square-dark": "Square & Dark"
							},
							"order": 1,
							"title": "Theme",
							"type": "string"
						},
						"trigger": {
							"default": "first-page-visit",
							"enum": [
								"first-page-visit",
								"button-click"
							],
							"enumNames": {
								"button-click": "Start the tour when a visitor clicks a button.",
								"first-page-visit": "Start the tour when a visitor arrives for the first time."
							},
							"format": "radios",
							"order": 2,
							"title": "Trigger",
							"type": "string"
						}
					}
				},
				"preview": {
					"handlers": [
						{
							"options": [
